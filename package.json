{
  "name": "plwgscreativeapparel-admin",
  "version": "1.0.0",
  "description": "Modern dark futuristic e-commerce admin dashboard for PlwgsCreativeApparel",
  "main": "index.html",
  "scripts": {
    "build:css": "npx @dhiwise/component-tagger && npx tailwindcss -i ./css/tailwind.css -o ./css/main.css",
    "watch:css": "npx @dhiwise/component-tagger && npx tailwindcss -i ./css/tailwind.css -o ./css/main.css --watch",
    "dev": "npm run watch:css",
    "start": "npm run build:css && node server.js",
    "serve": "node dev-server.js",
    "dev:full": "npm run build:css && node dev-server.js",
    "build": "npm run build:css",
    "clean": "rm -rf node_modules package-lock.json",
    "reinstall": "npm run clean && npm install"
  },
  "keywords": [
    "e-commerce",
    "admin-dashboard",
    "tailwindcss",
    "html",
    "javascript",
    "dark-theme",
    "futuristic-design"
  ],
  "author": "PlwgsCreativeApparel Team",
  "license": "MIT",
  "dependencies": {
    "@dhiwise/component-tagger": "^1.0.10",
    "@tailwindcss/forms": "^0.5.7",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "cloudinary": "^2.7.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "node-fetch": "^3.3.2",
    "nodemailer": "^7.0.5",
    "pg": "^8.16.3",
    "tailwindcss-animate": "^1.0.7",
    "tailwindcss-elevation": "^2.0.0",
    "tailwindcss-fluid-type": "^2.0.7"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/line-clamp": "^0.1.0",
    "@tailwindcss/typography": "^0.5.16",
    "tailwindcss": "^3.4.17"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mariaisabeljuarezgomez/PLWGCREATIVEAPPAREL.git"
  },
  "bugs": {
    "url": "https://github.com/mariaisabeljuarezgomez/PLWGCREATIVEAPPAREL/issues"
  },
  "homepage": "https://github.com/mariaisabeljuarezgomez/PLWGCREATIVEAPPAREL#readme"
}
